                                    removeBot(getBot().name)
                                    sleep(2000)
                                end
                                error()
                            end
                            tilePack = 0
                        else
                            for y,pack in pairs(packList) do
                                for i,tile in pairs(getTiles()) do
                                    if separated then
                                        if tile.fg == 0 or tile.bg == 0 then
                                            ::BACK::
