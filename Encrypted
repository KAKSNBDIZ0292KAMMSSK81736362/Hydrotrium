                        end
                        tilePack = 0
                    end
                else
                    infoid[world] = "NUKED !!!"
                    nuked = false
                    sleep(5000)
                    print("[ "..world.." ] packs Storage is NUKED, check ur storage!")
                    setting.botinfo("[ "..world.." ] packs Storage is NUKED, check ur storage! @everyone")
